                    echo 'building Agbash1 application.'
                }
            }
          stage("test"){
              steps {
                  echo 'testing Agbash1 application.'
              }
          }
          stage("deploy"){
              steps {
                  echo 'deploying Agbash1 application.'
             }
        } 
    }
}
